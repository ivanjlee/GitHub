<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".web.WebActivity"
    tools:parentTag="android.widget.FrameLayout">

    <com.github.design.widget.EmptyView
        android:id="@+id/empty_view"
        style="@style/EmptyViewStyle"
        app:title="@string/default_empty_title"
        android:layout_gravity="center"
        android:visibility="gone"
        tools:visibility="visible" />

    <ViewStub
        android:id="@+id/ssl_error_view"
        android:inflatedId="@+id/ssl_error_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout="@layout/bifrost_ssl_error_view"
        />

</merge>