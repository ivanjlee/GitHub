<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CirclePagerIndicator">
        <attr name="unselected_color" format="color" />
        <attr name="selected_color" format="color" />
        <attr name="stroke_width" format="boolean" />
    </declare-styleable>

    <declare-styleable name="EmptyView">
        <attr name="drawable" format="reference"/>
        <attr name="title" format="string"/>
        <attr name="message" format="string"/>
        <attr name="drawableSize" format="dimension"/>
    </declare-styleable>
</resources>